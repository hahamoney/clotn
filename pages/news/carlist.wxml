<!--pages/news/carlist.wxml-->

<swiper
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
>
  <block>
    <swiper-item>
      <image src="/image/banner1.jpeg" style="width:100%" class="slide-image"  mode="widthFix" />
    </swiper-item>
    <swiper-item>
      <image src="/image/banner4.png" style="width:100%" class="slide-image"  mode="widthFix" />
    </swiper-item>
    <swiper-item>
      <image src="/image/banner5.png" style="width:100%" class="slide-image"  mode="widthFix" />
    </swiper-item>
  </block>
</swiper>
<van-search
  placeholder="请输入搜索关键词"
  bind:search="onSearch"
/>

    <view class="news wbg"  wx:for="{{res}}"
     bindtap="branch" data-id="{{item.id}}">
      <van-row>
        <van-col span="5" custom-class="messageuser" >
          <image src="/image/cars.png" class="image_font"  mode="widthFix" />
          <text class='j_name'>{{item.j_name}}</text>
        </van-col>
        <van-col span="12" custom-class="messagetitle" >
          <view>
            <!-- <van-tag type="danger">置顶</van-tag> -->
            {{item.name}}
          </view> 

        </van-col>
        <van-col span="19">
          <view>
            <view class="rate">{{item.scope}}</view>
          </view>
        </van-col>
      </van-row>
    </view>

